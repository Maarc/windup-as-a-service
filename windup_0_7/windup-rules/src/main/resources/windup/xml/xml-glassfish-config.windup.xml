<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:windup="http://www.jboss.org/schema/windup"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.jboss.org/schema/windup http://www.jboss.org/schema/windup.xsd">

	<windup:pipeline type="XML" id="xml-glassfish-decorators">
		<windup:dtd-classification description="Glassfish Web Descriptor" effort="3" public-id-regex=".+GlassFish Application Server .+Servlet.+"/>
	</windup:pipeline>
</beans>
 